{
  "display_layout": "standard",
  "ocr_settings": {
    "dpi": 300,
    "language": "ces",
    "psm": 6
  },
  "patterns": {
    "invoice_number": "Trans\\s+(\\d+)",
    "date": "(\\d{2}\\.\\d{2}\\.\\d{2})\\s+\\d{2}:\\d{2}",
    "total_amount": "Celkem\\s+([\\d ,]+)",
    "table_start": "Srbická"
  },
  "table_columns": {
    "line_pattern": "^(?:[A-Z]\\s+)?([A-ZĚŠČŘŽÝÁÍÉÚŮĎŤŇĹ\\s]+?)\\s+(\\d{3,5})\\s+([\\d,]+)\\s+([A-Z])\\s*$",
    
    "ignore_patterns": [
      "^Celkem",
      "^DPH",
      "^ZAOKROUHLENÍ",
      "^Koruna",
      "^\\d+\\s+x\\s+",
      "^Obch",
      "^táhněte",
      "^ly fa",
      "^ho \\d+",
      "^-{3,}",
      "^={3,}",
      "^E:",
      "^\\d{2}\\.\\d{2}\\.\\d{2}"
    ]
  }
}

